<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="614.0" prefWidth="901.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/16" fx:controller="com.jam.jamcougstatistics.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TabPane prefHeight="598.0" prefWidth="861.0" tabClosingPolicy="UNAVAILABLE">
     <tabs>
         <Tab text="General 1-variable Stats">
           <content>
             <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
           </content>
         </Tab>
       <Tab text="Hypothesis Testing">
         <content>
           <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="546.0" prefWidth="861.0" />
         </content>
       </Tab>
       <Tab text="Untitled Tab 2">
         <content>
           <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="449.0" prefWidth="919.0" />
         </content>
       </Tab>
         <Tab text="Untitled Tab">
           <content>
             <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
           </content>
         </Tab>
     </tabs>
   </TabPane>

    <Label fx:id="welcomeText" />
   <Separator prefWidth="200.0" />
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
         <Label text="Data Slot: " />
         <RadioButton mnemonicParsing="false" text="1   " />
         <RadioButton mnemonicParsing="false" text="2" />
      </children>
   </HBox>
   <Button alignment="TOP_RIGHT" mnemonicParsing="false" onMouseClicked="#LoadDataClick" text="Load Data" />
</VBox>
